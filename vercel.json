{
    "build":[
        {
            "src":"weather_project/wsgi.py",
            "use":"@vercel/python",
            "config":{"maxLambdaSize":"15mb", "runtime":"python3.9"}
        }
    ],
    "route":[
        {
            "src":"/static/(.*)",
            "dest":"/static/$1"
        },
        {
            "src":"/(.*)",
            "dest":"weather_project/wsgi.py"
        }
    ]
}